{
    "name": "alikhalaj/sms",
    "description": "Package for SMS system with support for multiple gateways (SMS.ir, Kavenegar, RayanSms)",
    "type": "library",
    "version": "1.1.0",
    "license": "MIT",
    "keywords": [
        "sms",
        "laravel",
        "sms.ir",
        "kavenegar",
        "rayansms",
        "otp",
        "verification"
    ],
    "autoload": {
        "psr-4": {
            "Leenset\\Sms\\": "src/",
            "Leenset\\Sms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leenset\\Sms\\SmsServiceProvider8"
            ],
            "aliases": {
                "Sms": "Leenset\\Sms\\Sms"
            }
        }
    },
    "authors": [
        {
            "name": "alikhalaj",
            "email": "akh30002@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
